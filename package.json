{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "tsx ./src/server.ts",
    "dev-debug": "tsx --inspect-brk ./src/server.ts",
    "bundle-viz": "vite-bundle-visualizer",
    "build": "vite build && vite build --ssr",
    "lint": "eslint --fix 'src/**/*.@(mts|ts|tsx)'",
    "test": "vitest run --environment=node",
    "gen-eql": "pnpx @edgedb/generate edgeql-js --target ts --output-dir src/edgeql",
    "serve": "NODE_ENV=production tsx ./src/server.ts",
    "prepare": "husky install && pnpm run gen-eql"
  },
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "0.5.9",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.2",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "autoprefixer": "10.4.15",
    "daisyui": "3.5.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-solid": "0.12.1",
    "husky": "8.0.3",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "solid-devtools": "0.27.7",
    "tailwindcss": "3.3.3",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-bundle-visualizer": "0.10.0",
    "vite-plugin-checker": "0.6.1",
    "vite-plugin-inspect": "0.7.35",
    "vite-plugin-solid": "2.7.0",
    "vitest": "0.34.2"
  },
  "dependencies": {
    "@felte/solid": "1.2.10",
    "@solidjs/router": "0.8.3",
    "edgedb": "1.3.4",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.1",
    "solid-js": "1.7.11",
    "telefunc": "0.1.58"
  }
}
